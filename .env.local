# Example .env file
DEBUG=True
SECRET_KEY=key

# Database
DB_NAME=db-name
DB_USER=db-user
DB_PASSWORD=db-password
DB_HOST=db-host
DB_PORT=db-port

# Emails
EMAIL_BACKEND=django.core.mail.backends.smtp.EmailBackend
EMAIL_HOST=smtp.host
EMAIL_PORT=587
EMAIL_HOST_USER=user
