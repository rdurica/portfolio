# Cache
REDIS_LOCATION=redis://:password@127.0.0.1:6379/1
REDIS_KEY_PREFIX=prefix
CACHE_BACKEND=django_redis.cache.RedisCache

# AWS
AWS_ACCESS_KEY_ID=key
AWS_SECRET_ACCESS_KEY=key
AWS_STORAGE_BUCKET_NAME=key